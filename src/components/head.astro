		<meta property="twitter:site" content="@tombras"> 
		<meta property="twitter:creator" content="{Astro.twitter_username}"> 
		<meta property="title" content="{Astro.title}"> 
		<meta property="og:title" content="{Astro.title}"> 
		<meta property="twitter:title" content="{Astro.title}"> 
		<meta property="keywords" name="keywords" content="jtubert, John Tubert, Juan Tubert"> 
		<meta property="twitter:image" content="assets/ms-loexnabwvm.jpg"> 
		<meta property="og:image:url" content="assets/ms-loexnabwvm.jpg"> 
		<meta name="twitter:card" content="summary_large_image"> 
